public class test {
    public static void main(String[] args) {

    }

    public ListNode reverseList(ListNode head){
        ListNode a = head;
        ListNode left = null;
        ListNode right = a;

        while (right != null){
            right.next = left;
            left = right;
            right = right.next;
        }

        return a;
    }
}
class ListNode {
    int val;
    ListNode next = null;
}